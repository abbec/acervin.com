---
title: Portfolio
---

<% sort_portfolio(@item.children).each do |p_item| %>

<h1><%= p_item.first[:year] %></h1>

<% p_item.each do |item| %>

<h2><a href="<%= item.path %>"><%= item[:title] %></a></h2>
<p class="margin_left">
  <a href="<%= item.path %>">
   	<img src="<%= item.path + item[:image] %>" alt="<%= item[:title] %>" />
  </a>
</p>	 

<% end %>
<% end %>
